{
	"global": {
		"url": "https://www.cloudflare.com/api_json.html",
		"checkipv4": true,
		"checkipv6": true,
        "ipv4Site": "http://ip4.me",
		"ipv4Query": "td[bgcolor=D0D0F0]",
		"timeout": 5000,
		"strictSSL": true,
		"includes": {
			"tkn": "YOUR TOKEN",
			"email": "YOUR EMAIL"
		}
	},
	"domains": [
		{
			"name": "example.com",
			"a": "rec_edit",
			"type": "A",
			"ttl": 1,
			"service_mode": 1,
			"z": "example.com",
			"content": "{ipv4}",
			"id": "?"
		},
		{
			"name": "example2.com",
			"a": "rec_edit",
			"type": "AAAA",
			"ttl": 1,
			"service_mode": 1,
			"z": "example2.com",
			"content": "{ipv6}",
			"id": "?"
		}
	]
}
